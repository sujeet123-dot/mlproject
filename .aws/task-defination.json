  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": ["FARGATE"],
  "executionRoleArn": "arn:aws:iam::<your-account-id>:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "student-performance",
      "image": "PLACEHOLDER",  
      "portMappings": [
        {
          "containerPort": 80,
